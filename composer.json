{
    "name": "coreshop/market-warehouse-bundle",
    "type": "pimcore-bundle",
    "description": "CoreShop - Market Warehouse Bundle",
    "keywords": [
        "coreshop",
        "pimcore",
        "ecommerce"
    ],
    "homepage": "http://www.coreshop.org",
    "license": "proprietary",
    "authors": [
        {
            "name": "Dominik Pfaffenbauer",
            "email": "dominik@pfaffenbauer.at",
            "homepage": "https://www.pfaffenbauer.at/",
            "role": "Developer"
        }
    ],
    "require": {
        "coreshop/core-shop": "^3.0",
        "laminas/laminas-stdlib": "^3.3",
        "pimcore/pimcore": "^10.1.0"
    },
    "require-dev": {
        "behat/behat": "^3.3",
        "friends-of-behat/symfony-extension": "^2.1",
        "friends-of-behat/page-object-extension": "^0.3.2",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-doctrine": "^0.12",
        "phpstan/phpstan-symfony": "^0.12",
        "phpstan/phpstan-webmozart-assert": "^0.12",
        "behat/mink": "^1.8",
        "friends-of-behat/mink-extension": "^2.4",
        "lakion/mink-debug-extension": "^2.0",
        "friends-of-behat/mink-debug-extension": "^2.0",
        "phpunit/phpunit": "^9.5",
        "robertfausk/behat-panther-extension": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "CoreShop\\Bundle\\MarketWarehouseBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CoreShop\\Behat\\MarketWarehouseBundle\\": "tests/Behat"
        },
        "classmap": [
            "src/Kernel.php",
            "src/BehatKernel.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "pimcore": {
            "bundles": [
                "CoreShop\\Bundle\\MarketWarehouseBundle\\CoreShopMarketWarehouseBundle"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
